{
  "name": "chrisbarr.me",
  "version": "1.0.0",
  "description": "Chris Barr's Website",
  "author": "Chris Barr",
  "license": "none",
  "homepage": "https://chrisbarr.me",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FiniteLooper/chrisbarr.me.git"
  },
  "bugs": {
    "url": "https://github.com/FiniteLooper/chrisbarr.me/issues"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "lint": "ng lint",
    "update": "ncu -u && npm i",
    "dev:ssr": "ng run chrisbarr.me:serve-ssr",
    "serve:ssr": "node dist/chrisbarr.me/server/main.js",
    "build:ssr": "ng build && ng run chrisbarr.me:server",
    "build:routes": "node route-builder.js",
    "prerender": "npm run build:routes && ng run chrisbarr.me:prerender --routes-file routes.txt",
    "gh-deploy": "npm run lint && npm run prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.0.4",
    "@angular/cdk": "^17.0.1",
    "@angular/common": "^17.0.4",
    "@angular/compiler": "^17.0.4",
    "@angular/core": "^17.0.4",
    "@angular/forms": "^17.0.4",
    "@angular/platform-browser": "^17.0.4",
    "@angular/platform-browser-dynamic": "^17.0.4",
    "@angular/platform-server": "^17.0.4",
    "@angular/router": "^17.0.4",
    "@angular/ssr": "^17.0.2",
    "express": "^4.18.2",
    "hammerjs": "^2.0.8",
    "ng-gallery": "^11.0.0",
    "ngx-google-analytics": "^14.0.1",
    "tslib": "^2.6.2",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.1700.2",
    "@angular-devkit/build-angular": "^17.0.2",
    "@angular-eslint/builder": "17.1.0",
    "@angular-eslint/eslint-plugin": "17.1.0",
    "@angular-eslint/eslint-plugin-template": "17.1.0",
    "@angular-eslint/schematics": "17.1.0",
    "@angular-eslint/template-parser": "17.1.0",
    "@angular/cli": "~17.0.2",
    "@angular/compiler-cli": "^17.0.4",
    "@types/express": "^4.17.21",
    "@types/jasmine": "~5.1.3",
    "@types/node": "^20.9.3",
    "@typescript-eslint/eslint-plugin": "6.12.0",
    "@typescript-eslint/parser": "6.12.0",
    "bootstrap": "^5.3.2",
    "eslint": "^8.54.0",
    "typescript": "~5.2.2"
  }
}
